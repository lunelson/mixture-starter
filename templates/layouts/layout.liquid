<!doctype html>
<html lang="en">
    <head>

        <title>{% block title %}Empty{% endblock %}</title>

        {% include "head/mobile_meta" %}

        {{ "app.css" | asset_url | stylesheet_tag }}

        {% block style %}
        <style type="text/css" media="screen">
        </style>
        {% endblock %}

    </head>
    <body>

        <div id="wrapper">
            <!-- Include -->
            {% include "header" %}
            <!-- Content block, child page inject content -->
            {% block content %}{% endblock %}
        </div>

        {{ "app.js" | asset_url | script_tag }}

        {% block script %}
        <script type="text/javascript" charset="utf-8">
        </script>
        {% endblock %}

    </body>
</html>
